            ResultText.Text = "â³ Please wait, verification in progress..."
            return
        end
        
        if not TextBox then
            ResultText.Text = "âŒ Input error!"
            return
        end
        
        local inputText = TextBox.Text
        if not inputText or inputText == "" then
            ResultText.Text = "âš ï¸ Please enter a key first!"
            task.wait(2)
            ResultText.Text = ""
            return
        end
        
        -- Clean the input more thoroughly
        local inputKey = inputText:gsub("%s+", ""):gsub("[\r\n\t]", ""):gsub("^%s*(.-)%s*$", "%1")
        
        if #inputKey < 5 then
            ResultText.Text = "âš ï¸ Key too short! Please check your key."
            task.wait(2)
            ResultText.Text = ""
            return
        end
        
        -- Visual feedback
        ResultText.Text = "ðŸ”‘ Verifying key (Length: " .. #inputKey .. ")..."
        
        task.spawn(function()
            local verificationResult = verifyKey(inputKey)
            
            if verificationResult then
                ResultText.Text = "âœ… Key valid! Loading script..."
                TextBox.Text = ""
                
                -- Load script with error handling
                task.spawn(function()
                    task.wait(1)
                    local scriptSuccess, scriptError = pcall(function()
                        loadstring(game:HttpGet("https://raw.githubusercontent.com/danuup16/fs/refs/heads/main/fs"))()
                    end)
                    
                    if scriptSuccess then
                        onMessage("âœ… Script loaded successfully!")
                        -- Close GUI after successful load
                        task.wait(0.5)
                        if KeySystemGui then
                            KeySystemGui:Destroy()
                        end
                    else
                        ResultText.Text = "âŒ Error loading script!"
                        onMessage("âŒ Script loading error: " .. tostring(scriptError))
                        task.wait(3)
                        ResultText.Text = ""
                    end
                end)
            else
                ResultText.Text = "âŒ Invalid or expired key!"
                TextBox.Text = "" -- Clear invalid key
                task.wait(3)
                ResultText.Text = ""
            end
        end)
    end)

    -- FIXED: Enhanced Get Key button with better feedback
