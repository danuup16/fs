                task.wait(2)
                ResultText.Text = ""
            end
        end)
    end)
    
    -- Enhanced Close button
    CloseBtn.MouseButton1Click:Connect(function()
        if KeySystemGui then
            KeySystemGui:Destroy()
        end
    end)
    
    -- ADDED: Enter key support for TextBox
