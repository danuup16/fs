            -- Simulate Enter button click when Enter key is pressed
            EnterBtn.MouseButton1Click:Fire()
        end
    end)
    
    -- ADDED: Visual feedback for buttons
    local function addButtonEffect(button)
        button.MouseEnter:Connect(function()
            button.BackgroundTransparency = 0.5
        end)
        
        button.MouseLeave:Connect(function()
            button.BackgroundTransparency = 0.65
        end)
    end
    
    addButtonEffect(GetKeyBtn)
    addButtonEffect(EnterBtn)
    addButtonEffect(CloseBtn)
end)

-- ADDED: Additional debugging function to test KeyGuardian connection
local function testKeyGuardianConnection()
    task.spawn(function()
        task.wait(2) -- Wait for UI to load
        
        local success, result = pcall(function()
            if KeyGuardLibrary then
                onMessage("ðŸ”— KeyGuardian library loaded successfully!")
                
                -- Test getLink function
                if KeyGuardLibrary.getLink then
                    local link = KeyGuardLibrary.getLink()
                    if link then
                        onMessage("âœ… KeyGuardian link generation working!")
                        return true
                    else
                        onMessage("âš ï¸ KeyGuardian link generation returned nil!")
                        return false
                    end
                else
                    onMessage("âš ï¸ KeyGuardLibrary.getLink function not found!")
                    return false
                end
            else
                onMessage("âŒ KeyGuardian library is nil!")
                return false
            end
        end)
        
        if not success then
            onMessage("âŒ KeyGuardian connection error: " .. tostring(result))
        end
    end)
end
testKeyGuardianConnection()
end
